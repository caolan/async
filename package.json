    "babel-preset-es2015": "^6.3.13",
    "babelify": "^7.2.0",
    "benchmark": "bestiejs/benchmark.js",
    "bluebird": "^2.9.32",
    "chai": "^3.1.0",
    "coveralls": "^2.11.2",
    "es6-promise": "^2.3.0",
    "fs-extra": "^0.26.3",
    "gulp": "~3.9.0",
    "jscs": "^1.13.1",
