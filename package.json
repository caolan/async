{
  "name": "async",
  "description": "Higher-order functions and common patterns for asynchronous code",
  "main": "lib/async.js",
  "author": "Caolan McMahon",
  "version": "0.9.2",
  "keywords": [
    "async",
    "callback",
    "utility",
    "module"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/caolan/async.git"
  },
  "bugs": {
    "url": "https://github.com/caolan/async/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/caolan/async/raw/master/LICENSE"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "istanbul": "^0.3.13",
    "lodash": ">=2.4.1",
    "nodelint": ">0.0.0",
    "nodeunit": ">0.0.0",
    "uglify-js": "1.2.x"
  },
  "jam": {
    "main": "lib/async.js",
    "include": [
      "lib/async.js",
      "README.md",
      "LICENSE"
    ],
    "categories": [
      "Utilities"
    ]
  },
  "scripts": {
    "test": "nodeunit test/test-async.js",
    "coverage": "istanbul cover ./node_modules/nodeunit/bin/nodeunit -- test/test-async.js",
    "coveralls": "istanbul cover ./node_modules/nodeunit/bin/nodeunit -- test/test-async.js && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "spm": {
    "main": "lib/async.js"
  },
  "volo": {
    "main": "lib/async.js",
    "ignore": [
      "**/.*",
      "node_modules",
      "bower_components",
      "test",
      "tests"
    ]
  }
}
